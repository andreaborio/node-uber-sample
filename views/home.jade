extends layout

block content
    script(src='/javascripts/jquery.min.js')
    script(src='http://maps.google.com/maps/api/js?sensor=true')
    script(src='/javascripts/global.js')
    div.containter.text-center
        div.page-header
            h1 Welcome
        div
            h3 Where would you like  to be picked up?
